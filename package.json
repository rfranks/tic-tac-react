{
  "name": "tic-tac-react",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://rfranks.github.io/tic-tac-react",
  "author": {
    "email": "richardfranksjr@hotmail.com",
    "name": "Richard A Franks Jr"
  },
  "bugs": {
    "url": "https://github.com/rfranks/tic-tac-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rfranks/tic-tac-react.git"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.12",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "d3-array": "2.0.3",
    "d3-axis": "1.0.12",
    "d3-fetch": "1.1.2",
    "d3-scale": "2.2.2",
    "d3-selection": "1.4.0",
    "d3-shape": "1.3.3",
    "d3-time": "1.0.10",
    "d3-time-format": "2.1.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jsdom-global": "3.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "typescript": "^3.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --coverage --verbose=false",
    "test-debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|md|vue)\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "jsx-a11y",
      "prettier"
    ],
    "rules": {
      "semi": 1,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    }
  },
  "prettier": {
    "bracketSpacing": false,
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "eslintIgnore": [
    "build/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint && npm run test"
    }
  },
  "jest": {
    "bail": 1,
    "coverageDirectory": "./build/test-results/coverage",
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "preset": "jest-puppeteer",
    "reporters": [
      "default",
      [
        "jest-html-reporters",
        {
          "publicPath": "./build/test-results",
          "filename": "jest-results.html"
        }
      ]
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "verbose": true
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest-html-reporters": "^2.0.1",
    "jest-puppeteer": "^4.4.0",
    "jsdom": "16.2.2",
    "prettier": "^2.0.5",
    "puppeteer": "^4.0.1",
    "react-test-renderer": "^16.13.1"
  }
}
